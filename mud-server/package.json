{
  "name": "harris-wilderness-mud",
  "version": "1.0.0",
  "description": "Retro text-based MUD with OpenClaw autonomous integration",
  "main": "src/server.py",
  "scripts": {
    "start": "python src/server.py",
    "start:terminal": "python -m http.server 8080 --directory ../mud-terminal",
    "agent": "node ../integrations/mud-agent/agent.js",
    "dev": "concurrently \"npm run start\" \"npm run start:terminal\""
  },
  "keywords": ["mud", "text-adventure", "websocket", "openclaw"],
  "author": "Bruce Harris",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.14.2"
  }
}
